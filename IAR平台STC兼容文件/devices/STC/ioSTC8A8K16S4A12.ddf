;; Interrupt Vectors
[InterruptList]
;;-----------------------------------------------------------------------------
;;                           Vector Enable                                     
;;            Id                  Prio                Pending           Comment
;;-----------------------------------------------------------------------------
Interrupt0  = NMI            0x00 1                                            
Interrupt1  = INT0           0x03 2 IE.EX0            TCON.IE0         
Interrupt2  = Timer0         0x0B 2 IE.ET0            TCON.TF0         
Interrupt3  = INT1           0x13 2 IE.EX1            TCON.IE1          
Interrupt4  = Timer1         0x1B 2 IE.ET1            TCON.TF1          
Interrupt5  = ADC            0x2B 2 IE.EADC           ADC_CONTR.ADC_FLAG          
Interrupt6  = LVD            0x33 2 IE.ELVD           PCON.LVDF          
Interrupt7  = PCA            0x3B 2 CMOD.ECF          CMPCR1.CMPIF          
Interrupt8  = UART2          0x43 2 IE2.ES2           S2CON.S2RI         
Interrupt9  = SPI            0x4B 2 IE2.ESPI          SPSTAT.SPIF           
Interrupt10 = INT2           0x53 2 INTCLKO.EX2       AUXINTIF.INT2IF        
Interrupt11 = INT3           0x5B 2 INTCLKO.EX3       AUXINTIF.INT3IF        
Interrupt12 = Timer2         0x63 2 IE2.ET2           AUXINTIF.T2IF      
Interrupt16 = INT4           0x83 2 INTCLKO.EX4       AUXINTIF.INT4IF    
Interrupt17 = UART3          0x8B 2 IE2.ES3           S3CON.S3RI         
Interrupt18 = UART4          0x93 2 IE2.ES4           S4CON.S4RI           
Interrupt19 = Timer3         0x9B 2 IE2.ET3           AUXINTIF.T3IF        
Interrupt20 = Timer4         0xA3 2 IE2.ET4           AUXINTIF.T4IF        
Interrupt21 = CMP            0xAB 2 CMPCR1.PIE        CMPCR1.CMPIF         
Interrupt22 = PWM            0xB3 2 PWMCR.ECBI        PWMCFG.CBIF          
